<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SmartShelf.EditSmartShelfPage"
             Title="Edit SmartShelf">
    <StackLayout Orientation="Vertical" Padding="10,10,10,10">
        <Label Text="Smart Shelf Name" FontSize="Small"/>
        <Entry x:Name="shelfName" Text="{Binding Name, Mode=TwoWay}" Placeholder="" />
    </StackLayout>
</ContentPage>